import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int X = s.nextInt();
		int Y = s.nextInt();
		while (X!=Y){
		    if(X>Y){
		        System.out.println("Decrescente");
		        
		    } else {
		        System.out.println("Crescente");
		        
		    }
		    X = s.nextInt();
		    Y = s.nextInt();
		}
	}
}
