import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
    Scanner s = new Scanner(System.in);
	int inicio = s.nextInt();
	int fim = s.nextInt();
    if(fim>inicio){
        int tempo = fim - inicio;
        System.out.println("O JOGO DUROU "+tempo+" HORA(S)");
    }
    else if(inicio>fim){
        int tempo = (24-inicio) + fim;
        System.out.println("O JOGO DUROU "+tempo+" HORA(S)");
    }
    else{
        int tempo = 24;
        System.out.println("O JOGO DUROU "+tempo+" HORA(S)");
    }
    
    }
}
