import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int X = s.nextInt();
		if(X%2==0){
		    X+=1;
		    for(int i =1; i<7; i++){
		        System.out.println(X);
		        X+=2;
		    }
		} else {
		    for(int i =1; i<7; i++){
		        System.out.println(X);
		        X+=2;
		    }
		}
	}
}
