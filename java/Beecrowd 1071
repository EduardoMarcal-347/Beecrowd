import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int X = s.nextInt();
		int Y = s.nextInt();
		int soma =0;
		for(int i = X; i>X&&i<Y; i++){
		    if(i%2!=0){
		        soma+=i;
		    }
		}
		System.out.println(soma);
	}
}
