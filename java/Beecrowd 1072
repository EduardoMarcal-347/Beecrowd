import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int N = s.nextInt();
		int in = 0;
		int out = 0;
		for(int i =0; i<N; i++){
	        double X = s.nextDouble();
	        if(X>=10&&X<=20){
	            in++;
		    } else{
	            out++;
	        }
	    }
		System.out.println(in+" in\n"+out+" out");
	}
}
