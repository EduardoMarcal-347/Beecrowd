import java.util.Scanner;
import java.text.DecimalFormat;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		DecimalFormat deci = new DecimalFormat("0");
		int N = s.nextInt();
		double cub=0;
		double quad=0;
		for(int i = 1; i<=N; i++){
		    quad = Math.pow(i,2);
		    cub = Math.pow(i,3);
		    System.out.println(i + " " + deci.format(quad) + " " + deci.format(cub));
		}
	}
}
